{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": ["error", "never"],
    "space-before-function-paren": ["error", "always"],
    "no-console": ["error", { allow: ["warn", "error","log"] }],
    "react/jsx-filename-extension":[
      1,{
        "extensions": ["js","jsx"]
    }],
    "jsx-equals-spacing": [2, "always"],
    "no-param-reassign": 0
  }
}